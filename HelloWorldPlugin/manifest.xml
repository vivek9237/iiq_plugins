<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE Plugin PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Plugin certificationLevel="None" displayName="Hello World!" minSystemVersion="7.1" name="HelloWorld" version="1.0">
	<Attributes>
		<Map>
			<entry key="minUpgradableVersion" value="1.0" />
			<entry key="fullPage">
				<value>
					<FullPage title="Hello World!!!" />
				</value>
			</entry>
			<entry key="settings">
				<value>
					<List>
						<Setting dataType="string" helpText="Enter a message you want to print in console" label="Your Message" name="yourMessage" defaultValue="Hello World!!!"/>
					</List>
				</value>
			</entry>
			<entry key="snippets">
				<value>
					<List>
						<Snippet regexPattern=".*identities\/identities\.jsf.*" rightRequired="">
							<Scripts>
								<String>ui/js/header.js</String>
							</Scripts> 
						</Snippet>
					</List>
				</value>
			</entry> 
		</Map>
	</Attributes>
</Plugin>